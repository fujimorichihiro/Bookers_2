<%= form_tag(search_path, :method => 'get' ) do %>
  <%= text_field_tag :search %>
  <%= select_tag(:table, options_for_select([["User","1"],["Book","2"]]))%>
  <%= select_tag(:option, options_for_select([["完全一致","1"],["部分一致","2"],["前方一致","3"],["後方一致","4"]]))%>
  <%= submit_tag "検索" %>
<% end %>